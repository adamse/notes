.decl uses(lmod:symbol, lname:symbol, umod:symbol, uname:symbol)
.input uses

.decl exported(lmod:symbol, lname:symbol)
.input exported

.decl defined(lmod:symbol, lname:symbol)
.input defined

.decl graph(lmod:symbol, lname:symbol, umod:symbol, uname:symbol)
.output graph

graph(lmod, lname, umod, uname) :- uses(lmod, lname, umod, uname).
graph(lmod, lname, umod, uname) :- 
  graph(lmod, lname, a, b), 
  uses(a, b, umod, uname).

.decl unused(lmod:symbol, lname:symbol)
.output unused
unused(lmod, lname) :- 
  defined(lmod, lname), 
  !exported(lmod, lname), 
  !uses(_, _, lmod, lname).
